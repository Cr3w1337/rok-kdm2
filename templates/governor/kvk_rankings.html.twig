<div class="gov-rankings">
    <table class="table table-striped table-bordered shadow-sm">
        <thead>
        <tr>
            <th scope="col">KVK #</th>
            <th scope="col">Rank</th>
            <th scope="col">Contribution</th>
        </tr>
        </thead>
        <tbody>
        {% for kvkRanking in gov.kvkRankings %}
        <tr>
            <th scope="row">{{ kvkRanking.kvkNumber }}</th>
            <td>{{ kvkRanking.ranking|default('-') }}</td>
            <td>{% if kvkRanking.contribution > 0 %}{{ kvkRanking.contribution|number_format }}{% else %}-{% endif %}</td>
        </tr>
        {% endfor %}
        </tbody>
    </table>
</div>